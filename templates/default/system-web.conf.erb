# 
# $SPLUNK_HOME/etc/system/local/web.conf 
#
# Generated by Chef templates/default/system-web.conf.erb
# 

[settings]
<%-if ['deployserver', 'search'].include? node[:splunk][:type] %>
startwebserver=1
httpport=<%= node[:splunk][:webserver_options][:port] %>
updateCheckerBaseURL=<%= node[:splunk][:webserver_options][:update_checker_url] %>
<%-else %>
startwebserver=0
<%-end %>
<%-if node[:splunk][:ssl_options][:enable_ssl] %>
enableSplunkWebSSL=<%= node[:splunk][:ssl_options][:enable_ssl].inspect %>
privKeyPath=etc/auth/splunkweb/<%= @webserver_key %>
caCertPath=etc/auth/splunkweb/<%= @webserver_cert %>
<%-end %>
